  "dependencies": {
    "@sliphua/pjax": "^2.4.0",
    "@waline/client": "^2.7.1",
    "aplayer": "^1.10.1",
    "atomic-algolia": "^0.3.19",
    "autocomplete.js": "^0.38.1",
    "clipboard": "^2.0.11",
    "cookieconsent": "^3.1.1",
    "echarts": "^5.3.3",
